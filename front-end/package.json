{
  "name": "chat-front-end",
  "version": "0.0.5",
  "author": "zhangbo",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "eslint:comment": "使用 ESLint 检查并自动修复 src 目录下所有扩展名为 .js 和 .vue 的文件",
    "eslint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "prettier:comment": "自动格式化当前目录下的所有文件",
    "prettier": "prettier --write",
    "docker:local": "docker build -t chatfrontend:0.0.1 . && docker run --name chatfrontend -d -p 80:80 chatfrontend:0.0.1",
    "release": "pnpm run build && docker build -t chatfrontend:0.0.1 . && docker save chatfrontend:0.0.1 > ../release/chatfrontend.tar"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "pinia": "^2.1.4",
    "vant": "^4.6.0",
    "vconsole": "^3.15.1",
    "vue": "^3.2.47",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.15.0",
    "less": "^4.1.3",
    "prettier": "^2.8.8",
    "typescript": "^5.0.2",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.3.9",
    "vue-tsc": "^1.4.2"
  }
}
